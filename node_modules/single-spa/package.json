{
  "name": "single-spa",
  "version": "4.1.0",
  "description": "Multiple applications, one page",
  "main": "lib/umd/single-spa.js",
  "module:": "lib/esm/single-spa.js",
  "scripts": {
    "build": "rollup -c --environment NODE_ENV:'production'",
    "build:dev": "rollup -c",
    "build:analyze": "rollup -c --environment ANALYZER:'analyzer'",
    "watch": "rollup -c -w",
    "prepublish": "in-publish && yarn build || not-in-publish",
    "clean": "rm -rf lib",
    "test": "BABEL_ENV=test jest",
    "test:debug": "BABEL_ENV=test jest --watch",
    "lint": "eslint src"
  },
  "repository": "https://github.com/CanopyTax/single-spa",
  "keywords": [
    "single",
    "page",
    "application",
    "framework",
    "react",
    "angular",
    "vue"
  ],
  "author": "Joel Denning",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "custom-event": "^1.0.1",
    "eslint": "5.11.1",
    "eslint-config-canopy": "2.1.0",
    "in-publish": "^2.0.0",
    "jest": "^24.1.0",
    "rollup": "^1.1.2",
    "rollup-plugin-analyzer": "^3.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.4"
  },
  "browserslist": [
    "ie >= 11",
    "last 4 Safari major versions",
    "last 10 Chrome major versions",
    "last 10 Firefox major versions",
    "last 4 Edge major versions"
  ],
  "ignore": [
    "examples",
    "docs"
  ]
}
